<?xml version="1.0"?>

<robot name="SBR">

    <link name="base_link">
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>

        <visual>
            <geometry>
                <mesh filename="file:///$(find gazebo_visualization)/meshes/base_link.STL">
                    <scale>1 1 1</scale>
                </mesh>
            </geometry>

            <origin xyz="0 0 0.4" rpy="0 0 0"/>
        </visual>

        <collision>
            <geometry>
                <mesh filename="file:///$(find gazebo_visualization)/meshes/base_link.STL">
                    <scale>1 1 1</scale>
                </mesh>
            </geometry>

            <origin xyz="0 0 0.4" rpy="0 0 0"/>
        </collision>

    </link>

</robot>